{
  "name": "@soonit/plugin-patched-garfish",
  "description": "patch for @modern-js/plugin-garfish",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/runtime/plugin-garfish"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.44.0",
  "jsnext:source": "./src/cli/index.ts",
  "types": "./src/cli/index.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/runtime/index.d.ts"
      ],
      "cli": [
        "./dist/types/cli/index.d.ts"
      ],
      "runtime": [
        "./dist/types/runtime/index.d.ts"
      ],
      "deps": [
        "./dist/types/deps/index.d.ts"
      ],
      "types": [
        "./type.d.ts"
      ]
    }
  },
  "main": "./dist/cjs/cli/index.js",
  "module": "./dist/esm/runtime/index.js",
  "exports": {
    "./types": "./type.d.ts",
    ".": {
      "types": "./dist/types/runtime/index.d.ts",
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/cjs/cli/index.js"
    },
    "./cli": {
      "types": "./dist/types/cli/index.d.ts",
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/cjs/cli/index.js"
    },
    "./deps": {
      "types": "./dist/types/deps/index.d.ts",
      "jsnext:source": "./src/deps/index.ts",
      "default": "./dist/esm/deps/index.js"
    },
    "./runtime": {
      "types": "./dist/types/runtime/index.d.ts",
      "jsnext:source": "./src/index.ts",
      "default": "./dist/esm/runtime/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm & npm run build:esm-node & npm run build:cjs & npm run build:type",
    "build:esm": "tsc --outDir dist/esm --lib es5 --lib dom --module ESNext",
    "build:esm-node": "tsc --outDir dist/esm-node --lib es5 --lib dom --module ESNext",
    "build:cjs": "tsc --outDir dist/cjs --lib es5 --lib dom --module CommonJS",
    "build:type": "tsc --outDir dist/types --lib es5 --lib dom --emitDeclarationOnly --declaration"
  },
  "dependencies": {
    "@modern-js/utils": "2.43.0",
    "@types/debug": "4.1.7",
    "@types/react-loadable": "^5.5.6",
    "debug": "4.3.4",
    "garfish": "^1.8.1",
    "hoist-non-react-statics": "^3.3.2",
    "react-loadable": "^5.5.0",
    "@swc/helpers": "0.5.3"
  },
  "peerDependencies": {
    "@modern-js/runtime": "2.43.0",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@modern-js/app-tools": "2.44.0",
    "@modern-js/core": "2.44.0",
    "@modern-js/plugin-router-v5": "2.44.0",
    "@modern-js/runtime": "2.44.0",
    "@modern-js/tsconfig": "2.44.0",
    "@modern-js/types": "2.44.0",
    "@types/node": "^14",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "6.17.0",
    "typescript": "^5",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "types": "./dist/types/cli/index.d.ts"
  }
}
